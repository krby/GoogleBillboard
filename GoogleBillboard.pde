public boolean isPrime(double dNum)  
{   
  if (dNum < 2)
  {
    return false;
  }
    for (int p = 2; p <= Math.sqrt(dNum); p++)
    {
      if (dNum % p == 0)
      {
        return false;
      }
    }
    return true;  
} 

public boolean isSum49(String length10) //finds if sum of the length 10 str is 49
{
  int total = 0;
  for (int i = 0; i < 10 ; i++)
  {
    String charI = ""+length10.charAt(i); //turn each character in the str to an int
    int intI = Integer.parseInt(charI);
    total += intI;
    //println(intI);
  }

  if (total == 49)
  {
    return true;
  }
  return false;
}